{
    "extends": [
    	"airbnb",
    	"prettier",
    	"prettier/react"
    ],
    "plugins": [
    	// "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        // "ecmaVersion": 2017,
        // "sourceType": "module",
        // "ecmaFeatures": {
        //  "jsx": true
        // }
    },
    "env": {
    	// "es6": true,
    	"browser": true
    	// "node": true,
    	// "jest": true
    },
    "rules": {
        "no-var": "off",
        "vars-on-top": "off",
    	"no-console": "off",
    	"arrow-body-style": "off",
        // "jsx-a11y/click-events-have-key-events": "off",
        // "jsx-a11y/no-static-element-interactions": "off",
        "no-plusplus": "off"
    }
}